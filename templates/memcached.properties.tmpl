! Generated by confd {{datetime}}
{{range gets "/memcached/servers/*"}}
memcached.endpoints[{{base .Key}}]={{.Value}}
{{end}}

memcached.timeToLiveSeconds={{getv "/memcached/ttl" "10"}}

{{if exists "/memcached/uniqueid"}}
memcached.uniqueId={{getv "/memcached/uniqueid"}}
{{end}}

{{if exists "/memcached/elasticache"}}
memcached.aws.elasticache={{getv "/memcached/elasticache"}}
{{end}}

# This properties file contains all Memcached related configuration.
# If Memcached is not being used, this file is ignored.

# memcached.endpoints[i] has one server address and corresponding port on each line.
# 'i' is the index of the endpoint.
#
# Example:
# memcached.endpoints[0]=jss-memcached-1.com:11211
# memcached.endpoints[1]=jss-memcached-2.com:11211

# Time to live in seconds
# memcached.timeToLiveSeconds=120

# Unique id required when sharing memcached endpoints with two different JSS apps
# memcached.uniqueId=JAMF

# Whether you are using elasticache configuration endpoint
# memcached.aws.elasticache=false
